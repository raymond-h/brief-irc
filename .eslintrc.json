{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "kellyirc",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "require-yield": "off",
        "react/jsx-uses-vars": "error"
    },
    "plugins": [
        "react"
    ]
}
